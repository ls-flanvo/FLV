# Mapbox Token
NEXT_PUBLIC_MAPBOX_TOKEN=your_mapbox_token_here

# Supabase - Transaction Pooler (porta 6543)
DATABASE_URL="postgresql://postgres.<PROJECT_REF>:<PASSWORD>@aws-1-eu-central-1.pooler.supabase.com:6543/postgres?pgbouncer=true&connection_limit=10"

# Supabase - Direct Connection (porta 5432)
DIRECT_URL="postgresql://postgres:<PASSWORD>@db.<PROJECT_REF>.supabase.co:5432/postgres"

# JWT Secret (min 32 characters)
JWT_SECRET=your-super-secret-jwt-key-min-32-chars

# Stripe (Keys from https://dashboard.stripe.com)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_...
STRIPE_SECRET_KEY=sk_test_...

# Resend (Email service)
RESEND_API_KEY=re_...

# Flight API
AVIATION_STACK_API_KEY=your_api_key

# App URL
NEXT_PUBLIC_APP_URL=http://localhost:3000